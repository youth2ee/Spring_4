<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="qnaFilesMapper">
  
 	<insert id="fileWrite" parameterType="QnaFilesVO">
 		insert into qnafiles values (file_seq.nextval, #{num}, #{fname},#{oname})
 	</insert>
 	
 	<select id="fileList" parameterType="Integer" resultType="QnaFilesVO">
 		select * from qnafiles where num = #{num} 	
 	</select>
 	
 
  </mapper>